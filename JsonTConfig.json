[{
        "FileName": "appsettings.json",
        "Sections": {
            "DopAudit:EnableAuditing": true,
            "DopLogging:EnableFileLogger": {
                "Name": "Sujith",
                "Age": 30
            },
            "DopLogging:Serilog": {
                "Using": ["Serilog.Sinks.File"],
                "MinimumLevel": "Debug",
                "Properties": {
                    "Application": "SerilogSinkClient"
                }
            }
        }
    },
    {
        "FileName": "appsettings.Development.json",
        "Sections": {
            "DopLogging:EnableFileLogger": false,
            "DopLogging:Serilog": {
                "MinimumLevel": "Error",
                "Properties": {
                    "Application": "SerilogSinkClient"
                }
            }
        }
    }
]